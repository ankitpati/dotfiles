{
    "[markdown]": {
        "editor.wordWrap": "off"
    },
    "comments.useRelativeTime": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.acceptSuggestionOnEnter": "off",
    "editor.accessibilitySupport": "off",
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingQuotes": "never",
    "editor.autoSurround": "never",
    "editor.bracketPairColorization.enabled": true,
    "editor.cursorStyle": "block-outline",
    "editor.detectIndentation": false,
    "editor.dragAndDrop": false,
    "editor.emptySelectionClipboard": false,
    "editor.find.addExtraSpaceOnTop": false,
    "editor.fontFamily": "'Source Code Pro', 'Ubuntu Mono', 'CodeNewRomanNFM', monospace",
    "editor.inlayHints.enabled": "offUnlessPressed",
    "editor.inlayHints.fontSize": 9,
    "editor.largeFileOptimizations": true,
    "editor.lineNumbers": "off",
    "editor.linkedEditing": true,
    "editor.minimap.enabled": false,
    "editor.overviewRulerBorder": false,
    "editor.renderLineHighlight": "none",
    "editor.rulers": [
        72,
        80,
        100,
        120
    ],
    "editor.scrollBeyondLastLine": false,
    "editor.scrollPredominantAxis": false,
    "editor.suggest.preview": true,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "files.enableTrash": false,
    "files.eol": "\n",
    "files.exclude": {
        "**/.classpath": true,
        "**/.factorypath": true,
        "**/.project": true,
        "**/.settings": true
    },
    "files.hotExit": "off",
    "files.insertFinalNewline": true,
    "files.restoreUndoStack": false,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "git.openRepositoryInParentFolders": "never",
    "gitlens.codeLens.enabled": false,
    "gitlens.codeLens.scopes": [
        "blocks",
        "containers",
        "document"
    ],
    "gitlens.currentLine.dateFormat": "YYYY-MM-DDTHH:mm:ssZ",
    "gitlens.currentLine.format": "${email} ${date} ${message|50?}",
    "gitlens.currentLine.pullRequests.enabled": false,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.statusBar.enabled": false,
    "java.debug.settings.enableRunDebugCodeLens": false,
    "java.saveActions.organizeImports": true,
    "markdown.preview.fontFamily": "'Ubuntu', sans-serif",
    "markdown.preview.linkify": false,
    "markdown.validate.enabled": true,
    "mypy-type-checker.preferDaemon": true,
    "notebook.formatOnCellExecution": true,
    "prettier.arrowParens": "avoid",
    "prettier.bracketSameLine": true,
    "prettier.endOfLine": "lf",
    "prettier.proseWrap": "always",
    "prettier.tabWidth": 4,
    "prettier.vueIndentScriptAndStyle": true,
    "python.analysis.autoFormatStrings": false,
    "python.analysis.typeCheckingMode": "strict",
    "redhat.telemetry.enabled": false,
    "screencastMode.fontSize": 48,
    "search.followSymlinks": false,
    "settingsSync.ignoredSettings": [
        "sourcegraph.accessToken"
    ],
    "svg.completion.showAdvanced": true,
    "svg.preview.mode": "svg",
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.enablePersistentSessions": false,
    "terminal.integrated.localEchoLatencyThreshold": -1,
    "terminal.integrated.minimumContrastRatio": 1,
    "terminal.integrated.scrollback": 20000,
    "terminal.integrated.sendKeybindingsToShell": true,
    "terminal.integrated.smoothScrolling": true,
    "terminal.integrated.tabs.focusMode": "singleClick",
    "update.mode": "none",
    "vim.hlsearch": true,
    "vim.ignorecase": false,
    "vim.incsearch": true,
    "vim.surround": false,
    "vim.useCtrlKeys": true,
    "window.commandCenter": false,
    "window.density.editorTabHeight": "compact",
    "window.enableMenuBarMnemonics": false,
    "window.restoreWindows": "none",
    "window.titleBarStyle": "custom",
    "window.zoomLevel": 2,
    "workbench.activityBar.location": "hidden",
    "workbench.colorCustomizations": {
        "[Default High Contrast]": {
            "contrastActiveBorder": "#444",
            "contrastBorder": "#000",
            "editor.findMatchBackground": "#00f8",
            "editor.findMatchHighlightBackground": "#00f8",
            "editor.selectionBackground": "#4448",
            "editor.selectionForeground": "#fff",
            "editorBracketMatch.background": "#444",
            "editorBracketMatch.border": "#000",
            "editorGroup.border": "#000",
            "editorGroupHeader.border": "#000",
            "focusBorder": "#444",
            "input.border": "#000",
            "scrollbarSlider.activeBackground": "#4447",
            "scrollbarSlider.background": "#4445",
            "scrollbarSlider.hoverBackground": "#4446",
            "selection.background": "#4448",
            "sideBar.border": "#000",
            "sideBarSectionHeader.border": "#000",
            "statusBar.border": "#000",
            "tab.border": "#000",
            "terminal.selectionBackground": "#4448",
            "terminal.selectionForeground": "#fff",
            "titleBar.border": "#000",
            "window.activeBorder": "#000",
            "window.inactiveBorder": "#000"
        }
    },
    "workbench.colorTheme": "Default High Contrast",
    "workbench.editor.closeOnFileDelete": true,
    "workbench.editor.empty.hint": "hidden",
    "workbench.editor.focusRecentEditorAfterClose": false,
    "workbench.editor.limit.enabled": true,
    "workbench.editor.openPositioning": "last",
    "workbench.editor.restoreViewState": false,
    "workbench.editor.tabSizing": "shrink",
    "workbench.enableExperiments": false,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.sideBar.location": "right",
    "workbench.startupEditor": "newUntitledFile"
}
